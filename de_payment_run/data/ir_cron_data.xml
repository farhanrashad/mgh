<?xml version='1.0' encoding='utf-8'?>
<odoo noupdate="1">
    <record id="ir_cron_payment_run_scheduled" model="ir.cron">
        <field name="name">Payment Run: Execute Scheduled Payments</field>
        <field name="model_id" ref="model_account_payment_run"/>
        <field name="state">code</field>

        <field name="code">model._cron_execute_scheduled()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="active" eval="True"/>
        <field name="doall" eval="False"/>
    </record>
</odoo>
